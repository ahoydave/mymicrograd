   "source": [
    "from micrograd.engine import Value\n",
    "from nn import draw_dot"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "x1 = Value (2.0)\n",
    "x1.label = 'x1'\n",
    "f = x1 * x1\n",
    "f.label = 'f'\n",
    "\n",
    "x2 = Value(2.0)\n",
    "x2.label = 'x2'\n",
    "g = x2 ** 2\n",
    "g.label = 'g'\n",
    "\n",
    "f.backward()\n",
    "g.backward()"
   ]
  },
